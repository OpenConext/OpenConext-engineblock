{
    "name": "surfnet/openconextengineblock",
    "description": "Surfnet OpenConext SAML proxy",
    "type": "project",
    "keywords": ["saml", "proxy", "openconext", "SURFnet"],
    "homepage": "http://www.openconext.org",
    "license": "Apache-2.0",
    "support": {
        "email": "openconext-users@list.surfnet.nl",
        "issues": "https://github.com/OpenConext/OpenConext-engineblock/issues",
        "source": "https://github.com/OpenConext/OpenConext-engineblock"
    },
    "require": {
        "apache/shindig":"1094333",
        "cssmin/cssmin": "3.0.1",
        "dbpatch/dbpatch": "~1.2",
        "kriswallsmith/assetic": "1.2.*@dev",
        "mrclay/minify": "~2.1",
        "openconext/engineblock-fixtures": "*",
        "openid/php-openid": "dev-master#a287b2d85e753c84b3b883ed8ee3ffe8692c8477 as 2.2.2",
        "pimple/pimple": "~2.0",
        "simplesamlphp/saml2": "~0.4",
        "simplesamlphp/simplesamlphp": "~1.12",
        "zendframework/zendframework1":"~1.12"
    },
    "require-dev": {
        "behat/behat": "2.4.6",
        "behat/mink": "1.5.0",
        "behat/mink-extension": "1.1.4",
        "behat/mink-goutte-driver": "1.0.9",
        "phake/phake": "2.0.0-alpha2",
        "phpunit/phpunit": "3.7.19"
    },
    "scripts": {
        "pre-autoload-dump": [
            "bin/composer/dbpatch-post-install.sh",
            "bin/composer/simplesamlphp-post-install.sh",
            "bin/composer/zend-framework-post-install.sh",
            "bin/composer/shindig-post-install.sh"
        ]
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "apache/shindig",
                "version": "1094333",
                "source": {
                    "type": "svn",
                    "url": "http://svn.apache.org/repos/asf/shindig/trunk",
                    "reference": "php@1094333"
                },
                "autoload" : {
                    "classmap" : [
                        "external/OAuth",
                        "src"
                    ]
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "cssmin/cssmin",
                "version": "3.0.1",
                "dist": { "url": "http://cssmin.googlecode.com/files/cssmin-v3.0.1.php", "type": "file" },
                "autoload": { "classmap": [ "cssmin-v3.0.1.php" ] }
            }
        },
        {
            "type": "vcs",
            "url": "https://github.com/OpenConext/OpenConext-engineblock-fixtures"
        }
    ],
    "autoload": {
        "psr-0": {
            "DbPatch_": "vendor/dbpatch/dbpatch/src/",
            "EngineBlock_": "library/",
            "EngineBlock_Test": "tests/library/",
            "Grouper_": "library/",
            "Janus_": "library/",
            "OpenSocial_": "library/",
            "SurfConext_": "library/",
            "Surfnet_": "library/",
            "Authentication_":"application/modules/",
            "Cron_":"application/modules/",
            "Default_":"application/modules/",
            "Dummy_":"application/modules/",
            "Logout_":"application/modules/",
            "Profile_":"application/modules/",
            "Service_":"application/modules/",
            "Social_":"application/modules/"
        }
    }
}
