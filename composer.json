{
    "name": "openconext/engineblock",
    "description": "OpenConext SAML proxy",
    "type": "project",
    "keywords": ["saml", "proxy", "openconext"],
    "homepage": "http://www.openconext.org",
    "license": "Apache-2.0",
    "support": {
        "email": "info@openconext.org",
        "issues": "https://github.com/OpenConext/OpenConext-engineblock/issues",
        "source": "https://github.com/OpenConext/OpenConext-engineblock"
    },
    "require": {
        "php": "^8.2",
        "ext-dom": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "beberlei/assert": "^2.6",
        "composer/package-versions-deprecated": "^1.11",
        "doctrine/doctrine-bundle": "^2.7",
        "doctrine/annotations": "^1.14",
        "doctrine/dbal": "^2.13",
        "doctrine/doctrine-migrations-bundle": "^3.2.1",
        "doctrine/orm": "^2.14",
        "guzzlehttp/guzzle": "^6.3",
        "incenteev/composer-parameter-handler": "~2.0",
        "monolog/monolog": "~1.13",
        "openconext/monitor-bundle": "^3.1.0",
        "openconext/saml-value-object": "dev-feature/support-php-82",
        "pimple/pimple": "^3.5",
        "ramsey/uuid": "^3.3.0",
        "simplesamlphp/saml2": "^4.1",
        "symfony/cache": "v5.4.19",
        "symfony/debug-bundle": "v5.4.19",
        "symfony/expression-language": "v5.4.19",
        "symfony/flex": "^2.4",
        "symfony/form": "v5.4.19",
        "symfony/http-kernel": "^5.4",
        "symfony/mailer": "v5.4.19",
        "symfony/monolog-bundle": "v3.8.0",
        "symfony/security-bundle": "v5.4.20",
        "symfony/translation": "v5.4.19",
        "symfony/twig-bundle": "v5.4.19",
        "symfony/validator": "v5.4.19",
        "symfony/web-profiler-bundle": "v5.4.19",
        "twig/extensions": "^1.5",
        "twig/twig": "^2.0"
    },
    "require-dev": {
        "behat/mink": "^1.11",
        "behat/mink-browserkit-driver": "^2.2",
        "behat/mink-selenium2-driver": "^1.7",
        "friends-of-behat/mink-extension": "^2.5",
        "friends-of-behat/symfony-extension": "^2.1",
        "league/flysystem": "^2.5",
        "liip/functional-test-bundle": "^4.7.0",
        "malukenho/docheader": "^0.1.8",
        "mockery/mockery": "^1.3",
        "phake/phake": "^4.4",
        "phpcompatibility/php-compatibility": "^9.3",
        "phpmd/phpmd": "^2.13",
        "phpunit/phpunit": "^8.5",
        "squizlabs/php_codesniffer": "^3.7",
        "symfony/phpunit-bridge": "^5.0"
    },
    "replace": {
        "symfony/polyfill-mbstring": "1.99",
        "symfony/polyfill-php54": "1.99",
        "symfony/polyfill-php55": "1.99",
        "symfony/polyfill-php56": "1.99"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "autoload": {
        "psr-0": {
            "EngineBlock_": "library/",
            "EngineBlock_Test": "tests/library/",
            "Pdp_": "library/",
            "AttributeAggregation_": "library/",
            "SurfConext_": "library/",
            "Surfnet_": "library/"
        },
        "psr-4": {
            "OpenConext\\": "src/OpenConext"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "OpenConext\\": ["tests/unit/OpenConext/", "tests/integration/OpenConext/", "tests/functional/OpenConext/"]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "platform": {
            "php": "8.2"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "repositories": [
        {
            "url": "https://github.com/OpenConext/SamlValueObject.git",
            "type": "git"
        }
    ],
    "extra": {
        "symfony": {
            "require": "5.4.*"
        },
        "symfony-app-dir": "app",
        "symfony-var-dir": "app",
        "symfony-bin-dir": "app",
        "symfony-web-dir": "web",
        "incenteev-parameters": [
            {
                "file": "app/config/parameters.yml"
            },
            {
                "file": "app/config/functional_testing.yml"
            }
        ]
    }
}
